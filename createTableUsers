CREATE TABLE users
(
    Id SERIAL PRIMARY KEY,
    login CHARACTER VARYING(30) CHECK(login !=''),
    password CHARACTER VARYING(30) CHECK(password !=''),
    Age INTEGER NOT NULL,
    isDeleted boolean DEFAULT false
);
